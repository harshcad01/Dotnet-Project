<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MvcProject</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.rtl.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MvcProject.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"/>
</head>
<body>
<div class="d-flex" id="wrapper">
      <!-- Sidebar-->
    <div class="border-end bg-white" id="sidebar-wrapper">
        <div class="sidebar-heading border-bottom bg-danger text-light p-3">
          HMBS
        </div>


        <div class="list-group list-group-flush">
          <a
            class="list-group-item list-group-item-action list-group-item-drak custom-style p-2"
            href="#!"
            >DASHBOARD</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="pos.html"
            >POINT OF SALE</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="sell.html"
            >SELL</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="quatation.html"
            >QUOTATION</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="purchase.html"
            >PURCHASE</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="#!"
            >STOCK TRANSFER</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="product.html"
            >PRODUCT</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="#!"
            >CUSTOMER</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="#!"
            >SUPPLIER</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="#!"
            >ACCOUNTING</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="#!"
            >EXPENDITURE</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="#!"
            >REPORTS</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="#!"
            >ANALYTICS</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="#!"
            >SMS</a
          >
          <a
            class="list-group-item list-group-item-action list-group-item-dark p-2"
            href="#!"
            >USER</a
          >
        </div>
    </div>

    <!-- Page content wrapper-->
      
      <div id="page-content-wrapper">
        <!-- Top navigation-->
        <nav
          class="navbar navbar-expand-lg navbar-light bg-dark bg-opacity-75 text-light border-bottom mx-xxl-auto"
        >
          <div class="container-fluid">
            <h6 class="fw-lighter" id="show-time">Time</h6>
           
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <ul class="navbar-nav ms-auto mt-2 mt-lg-0">
                <li class="nav-item active">
                  <a class="nav-link text-light" href="home.html">HOME</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link text-light" href="about.html">ABOUT US</a>
                </li>
                <li class="nav-item dropdown">
                  <a
                    class="nav-link dropdown-toggle text-light"
                    id="navbarDropdown"
                    href="#"
                    role="button"
                    data-bs-toggle="dropdown"
                    aria-haspopup="true"
                    aria-expanded="false"
                    >OTHERS</a
                  >
                  <div
                    class="dropdown-menu dropdown-menu-end"
                    aria-labelledby="navbarDropdown"
                  >
                    <a class="dropdown-item" href="signin.html">SIGN IN</a>
                    <a class="dropdown-item" href="login.html">LOG IN</a>
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" href="#!">Something else here</a>
                  </div>
                </li>
              </ul>
            </div>
          </div>
        </nav>

 <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    
</div>
</div>
    
   

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - MvcProject - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <script src="js/myscript.js"></script>

    <!-- Core theme JS-->
    <script src="js/scripts.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
